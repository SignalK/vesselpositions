{
  "name": "@signalk/vesselpositions",
  "version": "1.2.0",
  "description": "Map showing vessel positions (also a demo of embedded components)",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode=production",
    "clean": "rimraf ./public",
    "create-release": "github-create-release --owner signalk --repository vesselpositions",
    "release": "git tag -d v$npm_package_version && git tag v$npm_package_version && git push --tags && git push && npm run create-release"
  },
  "keywords": [
    "signalk-embeddable-webapp"
  ],
  "author": "teppo.kurki@iki.fi",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-react": "^7.10.4",
    "@signalk/github-create-release": "^1.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^5.0.0-alpha.6",
    "leaflet": "^1.7.1",
    "observable-hooks": "^3.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.8.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.2.0"
  }
}
